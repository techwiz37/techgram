{
  "name": "@techgram/node",
  "version": "0.0.1",
  "description": "TypeScript/JavaScript library for building Telegram clients on Node.js and Bun",
  "keywords": [
    "telegram",
    "telegram-client",
    "telegram-bot",
    "telegram-api",
    "nodejs",
    "bun",
    "typescript",
    "esm",
    "commonjs"
  ],
  "homepage": "https://github.com/techwiz37/techgram#readme",
  "bugs": {
    "url": "https://github.com/techwiz37/techgram/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/techwiz37/techgram.git"
  },
  "license": "LGPL-3.0",
  "author": "Techwiz",
  "type": "module",
  "main": "./dist/cjs/mod.js",
  "module": "./dist/esm/mod.js",
  "types": "./mod.ts",
  "exports": {
    ".": {
      "types": "./mod.ts",
      "import": "./dist/esm/mod.js",
      "require": "./dist/cjs/mod.js",
      "default": "./dist/esm/mod.js"
    },
    "./worker": {
      "types": "./worker.ts",
      "import": "./dist/esm/worker.js",
      "require": "./dist/cjs/worker.js",
      "default": "./dist/esm/worker.js"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "node build.js",
    "postinstall": "node postinstall.js"
  },
  "dependencies": {
    "lru-cache": "^10.0.0",
    "mime-types": "^2.1.35",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "esbuild": "^0.19.12",
    "glob": "^10.5.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
